{
  "name": "radaiai",
  "displayName": "Food Facility Permits",
  "description": "Search and view food facility permits",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "permits.openSearchUI",
        "title": "Open Food Permits Search"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "watch": "npm run esbuild-base -- --watch"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.3.3"
  }
}
